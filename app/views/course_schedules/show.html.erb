<%= provide :title, @course_schedule.course_name %>
<div id="content-wrapper">
  <%= render "shared/top_heading", title: @course_schedule.course_name,
    page_name: t(".course_schedules") %>
  <div class="bg-1 tt-sidebar-right tt-overflow">
    <div class="container">
      <div class="row">
        <div class="col-md-9">
          <div class="tt-two-colors pright20">
            <%= render "courses/course_header", course: @course_schedule.course,
              course_schedule: @course_schedule %>
            <div class="simple-text size-4 color-9">
              <%= markdown @course_schedule.course.content %>
            </div>
            <div class="empty-space marg-lg-b35 marg-sm-b30"></div>
          </div>
        </div>
        <%= render "courses/right_sidebar_course" %>
      </div>
    </div>
  </div>
</div>
<div class="render-form-registration"></div>
