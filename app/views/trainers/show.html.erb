<%= provide :title, @trainer.name %>
<div id="breadcrumb" class="full-breadcrumb">
  <div class="container">
    <div class="header-page row">
      <div class="title-page col-md-6 col-sm-6">
        <h2><%= t ".title" %></h2>
      </div>
      <div class="breadcrumbs col-md-6 col-sm-6">
        <ul>
          <li><%= link_to t(".home"), root_path %></li>
          <li><%= link_to t(".title_heading"), trainers_path %></li>
          <li class="active"><span><%= t ".title" %></span></li>
        </ul>
      </div>
    </div>
  </div>
</div>

<div id="main-content" class="full-content">
  <div class="container">
    <div class="detail-member row">
      <%= render "trainer_info", trainer: @trainer %>
    </div>
  </div>
</div>
