<div class="bg-1">
  <div class="container">
    <div class="empty-space marg-lg-b85 marg-sm-b50 marg-xs-b30"></div>

    <div class="tt-heding type-2">
      <h2 class="tt-heding-title c-h4 txt-uppercase">
        <span><%= t ".position" %></span>
      </h2>
    </div>
    <div class="empty-space marg-lg-b60 marg-sm-b30"></div>

    <% trainer.user_positions.each do |position| %>
      <div class="tt-training clearfix">
        <div class="tt-training-date trainer-posiotion-date">
          <%= l position.start_time, format: :month_year_concise %>
          <span> - </span>
          <% if position.end_time %>
            <%= l position.end_time, format: :month_year_concise %>
          <% else %>
            <%= t ".now" %>
          <% end %>
        </div>
        <div class="tt-training-info trainer-posiotion-info">
          <div class="simple-text">
            <h4><%= position.position %></h4>
          </div>
        </div>
      </div>
      <div class="empty-space marg-lg-b30"></div>
    <% end %>
    <div class="empty-space marg-lg-b135 marg-md-b120 marg-sm-b100 marg-xs-b60"></div>

  </div>
</div>
