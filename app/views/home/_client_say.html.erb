<div class="container">
  <div class="empty-space marg-lg-b80 marg-sm-b50 marg-xs-b30"></div>
  <div class="row">
    <div class="col-md-7 about-faa-index">
      <div class="tt-heding type-2">
        <h4 class="tt-heding-title c-h4 txt-uppercase">
          <span><%= t ".about_us" %></span>
        </h4>
      </div>
      <div class="empty-space marg-lg-b40 marg-sm-b30"></div>

      <div class="tt-accordion type-1">
        <div class="tt-accordion-panel">
          <div class="tt-accordion-title active">
            <%= t ".our_mission" %><div class="tt-accordion-icon"></div>
          </div>
          <div class="tt-accordion-body active">
            <div class="tt-commercial">
              <div class="row">
                <div class="col-sm-4">
                  <%= image_tag "mission.png", class: "img-responsive" %>
                </div>
                <div class="col-sm-8">
                  <div class="simple-text size-3 color-3">
                    <p><%= t ".mission_text" %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="tt-accordion-panel">
          <div class="tt-accordion-title">
            <%= t ".our_vision" %><div class="tt-accordion-icon"></div>
          </div>
          <div class="tt-accordion-body">
            <div class="tt-commercial">
              <div class="row">
                <div class="col-sm-4">
                  <%= image_tag "vision.png", class: "img-responsive" %>
                </div>
                <div class="col-sm-8">
                  <div class="simple-text">
                    <p><%= t ".vision_text" %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="tt-accordion-panel">
          <div class="tt-accordion-title">
            <%= t ".core_value" %><div class="tt-accordion-icon"></div>
          </div>
          <div class="tt-accordion-body">
            <div class="tt-commercial">
              <div class="row">
                <div class="col-sm-4">
                  <%= image_tag "core_value.jpg", class: "img-responsive" %>
                </div>
                <div class="col-sm-8">
                  <div class="simple-text">
                    <p><%= t ".core_value_text" %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="empty-space marg-sm-b30"></div>
    </div>
    <div class="col-md-5">
      <div class="tt-heding type-2">
        <h4 class="tt-heding-title c-h4 txt-uppercase">
          <span><%= t ".client_said" %></span>
        </h4>
      </div>
      <div class="empty-space marg-lg-b40 marg-sm-b30"></div>
      <div class="tt-testimonal-slider slider-wth-thumbs">
        <div class="swiper-container thumbnails-preview" data-autoplay="0" data-loop="0" data-speed="500" data-center="0" data-slides-per-view="1">
          <div class="swiper-wrapper clearfix">
            <div class="swiper-slide active" data-val="0">
              <div class="tt-testimonal-slider-wrapper">
                <div class="simple-text size-4">
                  <p><%= t ".client1_said" %></p>
                </div>
                <div class="tt-testimonal-slider-name">- <%= t ".client1_name" %></div>
              </div>
            </div>
            <div class="swiper-slide" data-val="1">
              <div class="tt-testimonal-slider-wrapper">
                <div class="simple-text size-4">
                  <p><%= t ".client2_said" %></p>
                </div>
                <div class="tt-testimonal-slider-name">- <%= t ".client2_name" %></div>
              </div>
            </div>
            <div class="swiper-slide" data-val="2">
              <div class="tt-testimonal-slider-wrapper">
                <div class="simple-text size-4">
                  <p><%= t ".client3_said" %></p>
                </div>
                <div class="tt-testimonal-slider-name">- <%= t ".client3_name" %></div>
              </div>
            </div>
            <div class="swiper-slide" data-val="3">
              <div class="tt-testimonal-slider-wrapper">
                <div class="simple-text size-4">
                  <p><%= t ".client4_said" %></p>
                </div>
                <div class="tt-testimonal-slider-name">- <%= t ".client4_name" %></div>
              </div>
            </div>
          </div>
          <div class="pagination"></div>
        </div>
        <div class="swiper-container thumbnails" data-autoplay="0"
          data-loop="0" data-speed="500" data-center="1"
          data-slides-per-view="responsive" data-xs-slides="1"
          data-sm-slides="3" data-md-slides="3" data-lg-slides="3"
          data-add-slides="3">
          <div class="swiper-wrapper clearfix">
            <div class="swiper-slide current active" data-val="0">
              <div class="tt-testimonal-slider-nav">
                <div class="tt-testimonal-slider-user client-say-image">
                  <%= image_tag "clients_say/teamlab.png" %>
                </div>
              </div>
            </div>
            <div class="swiper-slide" data-val="1">
              <div class="tt-testimonal-slider-nav">
                <div class="tt-testimonal-slider-user client-say-image">
                  <%= image_tag "clients_say/bkhn.png" %>
                </div>
              </div>
            </div>
            <div class="swiper-slide" data-val="2">
              <div class="tt-testimonal-slider-nav">
                <div class="tt-testimonal-slider-user client-say-image">
                  <%= image_tag "clients_say/doan.jpg" %>
                </div>
              </div>
            </div>
            <div class="swiper-slide" data-val="3">
              <div class="tt-testimonal-slider-nav">
                <div class="tt-testimonal-slider-user client-say-image">
                  <%= image_tag "clients_say/dhda.jpg" %>
                </div>
              </div>
            </div>
          </div>
          <div class="pagination type-1 visible-xs-block"></div>
        </div>
      </div>
    </div>
  </div>
  <div class="empty-space marg-lg-b100 marg-sm-b50 marg-xs-b50"></div>
</div>
