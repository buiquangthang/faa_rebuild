<div class="bg-1">
  <div class="container">
    <div class="empty-space marg-lg-b90 marg-sm-b50 marg-xs-b30"></div>
    <div class="tt-heding type-2">
      <h4 class="tt-heding-title c-h4 txt-uppercase">
        <span><%= t ".our_clients" %></span>
      </h4>
    </div>
    <div class="empty-space marg-lg-b50 marg-sm-b30"></div>
    <div class="swiper-container" data-autoplay="3000" data-loop="1" data-speed="500"
      data-center="0" data-slides-per-view="responsive" data-xs-slides="2"
      data-sm-slides="3" data-md-slides="5" data-lg-slides="6" data-add-slides="6">
      <div class="swiper-wrapper clearfix">
        <div class="swiper-slide client-logo" data-val="0">
          <a href="https://www.hust.edu.vn/" target="_blank">
            <%= image_tag "our_clients/dai_hoc_bk_ha_noi.jpg" %></a>
        </div>
        <div class="swiper-slide client-logo" data-val="1">
         <a href="http://www.dut.udn.vn/" target="_blank">
            <%= image_tag "our_clients/dai_hoc_bk_da_nang.jpg" %></a>
        </div>
        <div class="swiper-slide client-logo" data-val="2">
          <a href="http://hus.vnu.edu.vn" target="_blank">
            <%= image_tag "our_clients/dai_hoc_khtn_ha_noi.jpg" %></a>
        </div>
        <div class="swiper-slide client-logo" data-val="3">
          <a href="http://donga.edu.vn/" target="_blank">
            <%= image_tag "our_clients/dai_hoc_dong_a.jpg" %></a>
        </div>
        <div class="swiper-slide client-logo" data-val="4">
          <a href="https://www.passerellesnumeriques.org/vi/" target="_blank">
            <%= image_tag "our_clients/pn-logo.png" %></a>
        </div>
        <div class="swiper-slide client-logo" data-val="5">
          <a href="http://www.hueic.edu.vn/" target="_blank">
            <%= image_tag "our_clients/cao_dang_cong_nghiep_hue.png" %></a>
        </div>
      </div>
      <div class="pagination type-1 visible-xs-block"></div>
    </div>
    <div class="empty-space marg-lg-b50 marg-sm-b30"></div>
  </div>
</div>
