<div class="tt-header-margin"></div>
<div id="content-wrapper">
  <%= render "shared/top_heading", title: t(".detail_course"),
    page_name: t(".course") %>
  <div class="bg-1 tt-sidebar-right tt-overflow">
    <div class="container">
      <div class="row">
        <div class="col-md-9">
          <div class="tt-two-colors pright20">
            <%= render "course_header", course: @course %>
            <div class="simple-text size-4 color-9">
              <%= markdown @course.content %>
            </div>
            <div class="empty-space marg-lg-b35 marg-sm-b30"></div>
          </div>
        </div>
        <%= render "right_sidebar_course" %>
      </div>
    </div>
  </div>
</div>
<div class="render-form-registration"></div>
